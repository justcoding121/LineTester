<Window x:Class="LineTester.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LineTester"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Grid Name="myGrid" Grid.Row="0">
        </Grid>
        <StackPanel Orientation="Vertical" Grid.Row="1" VerticalAlignment="Bottom" >
            <Button Height="30px"  Name="Test" Content="Test" Click="Test_Click"></Button>
            <Button Height="30px"  Name="Redo" Content="Redo" Click="Redo_Click"></Button>
        </StackPanel>
       
    </Grid>
    
</Window>
